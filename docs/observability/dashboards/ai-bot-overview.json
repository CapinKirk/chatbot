{
  "uid": "ai-overview",
  "title": "AI-bot Overview",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "stat",
      "title": "RPS",
      "gridPos": {"h":4,"w":6,"x":0,"y":0},
      "targets": [
        {"expr": "sum(rate(http_requests_total{service=\"ai-bot\"}[1m]))"}
      ]
    },
    {
      "type": "timeseries",
      "title": "Latency P95",
      "gridPos": {"h":8,"w":12,"x":6,"y":0},
      "targets": [
        {"expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{service=\"ai-bot\"}[5m])) by (le,route,method))"}
      ]
    },
    {
      "type": "timeseries",
      "title": "Latency P99",
      "gridPos": {"h":8,"w":12,"x":0,"y":4},
      "targets": [
        {"expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket{service=\"ai-bot\"}[5m])) by (le,route,method))"}
      ]
    }
  ]
}